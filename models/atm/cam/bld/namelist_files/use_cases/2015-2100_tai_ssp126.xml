<sfc_a1_ext_file   chem="trop_snap">CMIP6-Input/SSP-TaiESM/ssp126/emissions-cmip6-ScenarioMIP_IAMC-IMAGE-ssp126-1-1_sfc_so4_a1_anthro-ene_vertical_mol_175001-210101_0.9x1.25_c20190225.nc</sfc_a1_ext_file>
